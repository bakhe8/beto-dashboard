{
    "name":  "betodashboard",
    "private":  true,
    "version":  "1.0.0",
    "type":  "module",
    "packageManager":  "npm@10.9.0",
    "scripts":  {
                    "dev":  "turbo run dev --parallel",
                    "dev:demo":  "turbo run dev --filter=betodashboard-demo",
                    "build":  "turbo run build",
                    "build:core":  "turbo run build --filter=@betodashboard/core",
                    "analyze":  "vite build --mode analyze",
                    "preview":  "vite preview",
                    "test":  "turbo run test",
                    "test:core":  "turbo run test --filter=@betodashboard/core",
                    "test:e2e":  "playwright test",
                    "test:e2e:clean":  "npm run ports:kill \u0026\u0026 playwright test",
                    "report:e2e":  "playwright show-report --port 0",
                    "test:coverage":  "vitest run --coverage",
                    "lint":  "eslint .",
                    "format":  "prettier --write .",
                    "typecheck":  "tsc --noEmit",
                    "audit:prod":  "npm audit --production",
                    "docs:dev":  "vitepress dev docs",
                    "docs:build":  "vitepress build docs",
                    "docs:preview":  "vitepress preview docs",
                    "storybook":  "storybook dev -p 6006",
                    "build-storybook":  "storybook build",
                    "demo:dev:5174":  "vite --config examples/betodashboard-demo/vite.config.ts --port 5174",
                    "demo:build":  "vite build --config examples/betodashboard-demo/vite.config.ts",
                    "demo:preview:5174":  "npm run demo:build \u0026\u0026 vite preview --config examples/betodashboard-demo/vite.config.ts --port 5174 --strictPort",
                    "demo:preview:5173":  "npm run demo:build \u0026\u0026 vite preview --config examples/betodashboard-demo/vite.config.ts --port 5173 --strictPort",
                    "ports:kill":  "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/kill-ports.ps1",
                    "ports:kill:sh":  "bash scripts/kill-ports.sh",
                    "clean":  "rimraf dist examples/betodashboard-demo/dist storybook-static .turbo \"**/.vite\" coverage playwright-report test-results stats.html",
                    "reset":  "npm run clean \u0026\u0026 rimraf node_modules \u0026\u0026 npm ci",
                    "bootstrap:ps1":  "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/bootstrap_betodashboard.ps1 -InPlace",
                    "bootstrap:sh":  "bash scripts/bootstrap_betodashboard_inplace.sh",
                    "prepare":  "husky",
                    "serve:prod":  "node scripts/serve.js",
                    "demo:skeleton":  "node scripts/serve-skeleton.mjs",
                    "demo:record":  "node scripts/record-skeleton.mjs",
                    "test:e2e:prod":  "npm run build \u0026\u0026 playwright test -c playwright.config.prod.ts",
                    "metrics:collect":  "node scripts/collect-metrics.mjs",
                    "metrics:report":  "type test-results\\metrics.json",
                    "morph:component":  "node scripts/morph/create-component.mjs",
                    "ports:who":  "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/ports-who.ps1",
                    "demo:preview:random":  "npm run demo:build \u0026\u0026 vite preview --config examples/betodashboard-demo/vite.config.ts --port 0"
                },
    "workspaces":  [
                       "packages/*",
                       "examples/*"
                   ],
    "devDependencies":  {
                            "@axe-core/playwright":  "^4.9.1",
                            "@playwright/test":  "^1.45.0",
                            "@storybook/addon-essentials":  "^8.3.6",
                            "@storybook/html":  "^10.0.3",
                            "@storybook/html-vite":  "^8.3.6",
                            "@testing-library/dom":  "^10.1.0",
                            "@types/node":  "^20.14.2",
                            "@typescript-eslint/eslint-plugin":  "^8.46.2",
                            "@typescript-eslint/parser":  "^8.46.2",
                            "@vitest/coverage-v8":  "^4.0.6",
                            "axe-core":  "^4.9.1",
                            "eslint":  "^9.4.0",
                            "express":  "^4.21.1",
                            "helmet":  "^7.1.0",
                            "husky":  "^9.0.11",
                            "jsdom":  "^24.1.0",
                            "lint-staged":  "^15.2.5",
                            "prettier":  "^3.3.1",
                            "rimraf":  "^5.0.10",
                            "rollup-plugin-visualizer":  "^5.12.0",
                            "start-server-and-test":  "^2.1.2",
                            "tailwindcss":  "^3.4.13",
                            "turbo":  "^2.0.0",
                            "typescript":  "^5.4.5",
                            "vite":  "^7.1.12",
                            "vitepress":  "^1.5.0",
                            "vitest":  "^4.0.6"
                        },
    "dependencies":  {
                         "autoprefixer":  "^10.4.19",
                         "dompurify":  "^3.1.5",
                         "postcss":  "^8.4.38",
                         "postcss-nesting":  "^12.1.5",
                         "zod":  "^3.23.8"
                     },
    "overrides":  {
                        "esbuild": "^0.25.0"
                  },
    "lint-staged":  {
                        "*.{js,ts,css,md}":  "prettier --write"
                    }
}
